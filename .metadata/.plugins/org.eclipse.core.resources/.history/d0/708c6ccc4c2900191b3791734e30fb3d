package com.selenium.demo;

public class Demo1 {
	int a;
	int b;
	int a1;
	int b1;
	public void sub(int a, int b){
		int c=0;
		c=a-b;
	System.out.println("Subration of  "  +a +" and " +b + " is " +c);
	}
	public void add(int a1, int b1){
		int c1=0;
		c1=a1+b1;
		System.out.println("addition value" +c1);
	}
public static void main(String[] args){
	Demo1 demo=new Demo1();	
	demo.sub(20,10);
	demo.add(20,30);
	
}
}
